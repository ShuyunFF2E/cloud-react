{
	"name": "cloud-react",
	"version": "0.0.47",
	"description": "数据赢家 react 组件",
	"main": "cloud-react.js",
	"scripts": {
		"precommit": "npm run lint",
		"lint": "eslint src",
		"webpack": "webpack",
		"webpack-dev-server": "webpack-dev-server",
		"gen-index": "node ./scripts/generate-index.js",
		"start": "npm run gen-index && npm run webpack-dev-server -- --env.mode development --env.domain develop",
		"build": "npm run gen-index && npm run webpack -- --env.mode production --env.domain production",
		"site": "npm run gen-index && npm run webpack -- --env.mode site --env.domain production",
		"debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js --env.mode production",
		"preversion": "npm run lint",
		"version": "npm run build && npm publish dist",
		"postversion": "git push && git push --tags",
		"new:component": "cp -r tpls/new-component src/components/new-component"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ShuyunFF2E/ccms-components-react.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@babel/cli": "7.0.0-rc.1",
		"@babel/core": "7.0.0-rc.1",
		"@babel/plugin-proposal-class-properties": "^7.0.0-rc.1",
		"@babel/plugin-proposal-decorators": "7.0.0-rc.1",
		"@babel/plugin-proposal-export-namespace-from": "^7.0.0-rc.1",
		"@babel/plugin-proposal-function-sent": "^7.0.0-rc.1",
		"@babel/plugin-proposal-numeric-separator": "^7.0.0-rc.1",
		"@babel/plugin-proposal-throw-expressions": "^7.0.0-rc.1",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-transform-runtime": "7.0.0-rc.1",
		"@babel/preset-env": "7.0.0-rc.1",
		"@babel/preset-react": "^7.0.0",
		"@babel/runtime": "^7.4.5",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.0-beta.4",
		"clean-webpack-plugin": "^1.0.1",
		"copy-webpack-plugin": "^4.6.0",
		"css-loader": "^2.1.0",
		"eslint": "^5.16.0",
		"eslint-config-airbnb": "^17.1.0",
		"eslint-config-prettier": "^4.3.0",
		"eslint-import-resolver-webpack": "^0.11.1",
		"eslint-loader": "^2.1.2",
		"eslint-plugin-import": "^2.17.3",
		"eslint-plugin-jsx-a11y": "^6.2.1",
		"eslint-plugin-prettier": "^3.1.0",
		"eslint-plugin-react": "^7.13.0",
		"file-loader": "^3.0.1",
		"front-matter": "^3.0.2",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^1.3.1",
		"less": "^3.9.0",
		"less-loader": "^4.1.0",
		"lint-staged": "^10.0.8",
		"markdown-it": "^8.4.2",
		"mini-css-extract-plugin": "^0.5.0",
		"optimize-css-assets-webpack-plugin": "^5.0.1",
		"prettier": "^1.17.1",
		"pretty-quick": "^2.0.1",
		"style-loader": "^0.23.1",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"url-loader": "^1.1.2",
		"webpack": "^4.29.5",
		"webpack-bundle-analyzer": "^3.0.4",
		"webpack-cli": "^3.2.3",
		"webpack-dev-server": "^3.2.1",
		"webpack-merge": "^4.2.1"
	},
	"dependencies": {
		"classnames": "^2.2.6",
		"gridmanager-react": "^0.4.0",
		"jeasy": "0.0.12",
		"prop-types": "^15.7.2",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-router-dom": "^5.0.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"post-commmit": "git update-index -g"
		}
	},
	"lint-staged": {
		"src/**/*.{js,less}": [
			"pretty-quick —-staged",
			"git add"
		]
	},
	"bugs": {
		"url": "https://github.com/ShuyunFF2E/ccms-components-react/issues"
	},
	"homepage": "https://github.com/ShuyunFF2E/ccms-components-react#readme"
}
